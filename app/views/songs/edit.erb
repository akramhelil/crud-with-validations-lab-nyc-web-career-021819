<h2>Edit My Song</h2>
<%= form_for @song do |f| %>
<%= label_tag 'title:' %>
  <%= f.text_field :title %><br>
<%= label_tag 'released:' %>
  <%= f.check_box :released %><br>
<%= label_tag 'released_year:' %>
  <%= f.date_field :released_year %><br>
<%= label_tag 'artist_name:' %>
  <%= f.text_field :artist_name %><br>
<%= label_tag 'genre:' %>
  <%= f.text_field :genre %><br>
  <%= f.submit %><br>

<% end %>
